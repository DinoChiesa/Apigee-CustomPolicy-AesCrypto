<RaiseFault name="RF-BadRequest">
  <DisplayName>RF-BadRequest</DisplayName>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <FaultResponse>
   <Set>
     <Payload contentType="application/json">{
   "error" : "Bad Request.",
   "links" : {
     "encrypt" : "POST /encrypt1?passphrase=ABCDEFG   ...PAYLOAD...",
     "decrypt" : "POST /decrypt1?passphrase=ABCDEFG&amp;iv=diudiu     ...PAYLOAD..."
   }
}
</Payload>
     <StatusCode>404</StatusCode>
     <ReasonPhrase>Not Found</ReasonPhrase>
   </Set>
 </FaultResponse>
</RaiseFault>
